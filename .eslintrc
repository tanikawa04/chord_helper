{
    "extends": "eslint:recommended",
    "plugins": [],
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "rules": {
        "arrow-parens": 1,
        "arrow-spacing": 1,
        "constructor-super": 2,
        "eqeqeq": 2,
        "generator-star-spacing": 1,
        "no-class-assign": 2,
        "no-console": 0,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2,
        "no-unused-vars": 1,
        "no-var": 2,
        "object-shorthand": 1,
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "prefer-spread": 2,
        "quotes": [1, "single"],
        "require-yield": 2,
        "semi": 1
    }
}
